{
    "name": "shreejan/customize-dashboard-widget",
    "version": "1.0.0",
    "description": "A Filament plugin for customizable dashboard widgets with drag & drop",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shreejan Pandit",
            "email": "shreezanpandit@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Shreejan\\CustomizeDashboardWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shreejan\\CustomizeDashboardWidget\\CustomizeDashboardWidgetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}